{
    "name": "spatie/laravel-model-cleanup",
    "description": "This package deletes unneeded records in a database.",
    "keywords": [
        "spatie",
        "laravel-model-cleanup"
    ],
    "homepage": "https://github.com/spatie/laravel-model-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.4",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "orchestra/testbench":"^5.0",
        "mockery/mockery": "^1.0",
        "spatie/test-time": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ModelCleanup\\Test\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ModelCleanup\\ModelCleanupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
