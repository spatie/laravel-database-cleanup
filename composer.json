{
    "name": "spatie/laravel-model-cleanup",
    "description": "This package deletes unneeded records in a database.",
    "keywords": [
        "spatie",
        "laravel-model-cleanup"
    ],
    "homepage": "https://github.com/spatie/laravel-model-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "~5.2.0|~5.3.0|~5.4.0",
        "nesbot/carbon": "^1.0",
        "nikic/php-parser":"^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "orchestra/testbench":"^3.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ModelCleanup\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ModelCleanup\\ModelCleanupServiceProvider"
            ]
        }
    }
}
